<script src="http://localhost/Racine_Site/javascripts/jquery-1.10.0.min.js"></script>
<script>
    var Racine_Site = "http://localhoste/Racine_Site/vue/cpro";
    $(document).ready(function(){
      

        createCookie('cookieJBH',"Hello JB", '2');
        alert ("Cookie JBH active!");
    });

    function createCookie(name,value,days){
        var expires;
        if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days * 24 * 60 * 60 *1000));
            expires="; expires="+date.toGMTString();
        }else{
            expires="";
        }
    document.cookie = escape(name)+"="+escape(value)+expires+"; path=/; domain=http://localhost/Racine_Site/vue/cpro/";
    }
    createCookie('cookieJBH0',"Hello JB", '2');
    alert ("Cookie JBH active!2");
</script>


